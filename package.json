{
    "name": "discord-pidor-bot",
    "version": "1.0.0",
    "private": true,
    "bin": "bin/index.js",
    "main": "build/bot.js",
    "files": ["bin", "build"],
    "scripts": {
        "clear": "rimraf build",
        "dev": "tsx src/index.ts",
        "start": "node bin/index.js",
        "build": "esbuild --bundle src/index.ts --platform=node --target=node22 --outfile=build/bot.js --minify --keep-names --sourcemap=external",
        "prettify": "biome check --write .",
        "typecheck": "tsc --noEmit",
        "prepare": "husky"
    },
    "dependencies": {
        "@discordjs/formatters": "0.5.0",
        "@prisma/client": "6.3.0",
        "discord.js": "14.17.3",
        "dotenv": "16.4.7",
        "pino": "9.5.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/dotenv": "8.2.3",
        "@types/node": "22.10.2",
        "@yarnpkg/pnpify": "4.1.3",
        "esbuild": "0.24.2",
        "husky": "9.1.7",
        "prisma": "6.3.0",
        "rimraf": "6.0.1",
        "tsx": "4.19.2",
        "typescript": "5.7.2"
    },
    "prisma": {
        "schema": "src/data/schema.prisma"
    },
    "packageManager": "yarn@4.6.0"
}
