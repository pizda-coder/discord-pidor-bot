{
    "name": "discord-pidor-bot",
    "version": "1.0.0",
    "private": true,
    "bin": "bin/index.js",
    "main": "build/bot.js",
    "files": [
        "bin",
        "build"
    ],
    "scripts": {
        "clear": "rimraf build",
        "dev": "tsx src/index.ts",
        "start": "node bin/index.js",
        "build": "esbuild --bundle src/index.ts --platform=node --target=node22 --outfile=build/bot.js --minify --keep-names --sourcemap=external",
        "prettify": "biome check --write .",
        "typecheck": "tsc --noEmit",
        "prepare": "husky"
    },
    "dependencies": {
        "@discordjs/formatters": "0.6.1",
        "@prisma/client": "6.9.0",
        "discord.js": "14.19.3",
        "dotenv": "16.5.0",
        "express": "5.1.0",
        "pino": "9.7.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/dotenv": "8.2.3",
        "@types/express": "5.0.3",
        "@types/node": "22.15.30",
        "@yarnpkg/pnpify": "4.1.5",
        "esbuild": "0.25.5",
        "husky": "9.1.7",
        "prisma": "6.9.0",
        "rimraf": "6.0.1",
        "tsx": "4.19.4",
        "typescript": "5.7.2"
    },
    "prisma": {
        "schema": "src/data/schema.prisma"
    },
    "packageManager": "yarn@4.6.0"
}
